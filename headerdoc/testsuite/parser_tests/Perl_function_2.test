                                        }
                                } else {
                                        my $newpartnl = $newpart;
                                        my $newpartnlcount = ($newpartnl =~ tr/\n//);
                                        my $partnl = $part;
                                        my $partnlcount = ($partnl =~ tr/\n//);
                                        my $nlchange = ($newpartnlcount - $partnlcount);
                                        print STDERR "NLCHANGE: $nlchange (FILEOFFSET = $fileoffset)\n" if ($offsetDebug);
