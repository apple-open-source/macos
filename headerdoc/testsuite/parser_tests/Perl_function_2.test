                                                $noCPPThisToken = 1;
                                        }
                                } elsif ($parserState->{NEXTTOKENNOCPP} == 3) {
                                        # We're in an "if" block, so first word token
                                        # drops us back to default "if" block state.
                                        if ($part !~ /(\s|\()/) {
