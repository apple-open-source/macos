                }
                print STDERR "INMACROTAIL: $inMacroTail\n" if ($cppDebug);

                my @cpptrees;
                my $cpptreecur = HeaderDoc::ParseTree->new();
                my $cpptreetop = $cpptreecur;

                # print STDERR "CHECK LINE $line\n";
                if ($line =~ /^\s*#include (.*)$/) {
