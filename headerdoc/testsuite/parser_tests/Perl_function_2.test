                                                $skip = 1;

                                                $inParen++;
                                                push(@cpptrees, $cpptreecur);
                                                $cpptreecur = $cpptreecur->firstchild(HeaderDoc::ParseTree->new());
                                        } elsif ($part eq ")") {
