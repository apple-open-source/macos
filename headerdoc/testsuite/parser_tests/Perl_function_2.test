                }
                my $cppleaddebug = 0;
                do {
                    my $pos = 0;
                    my $dropargs = 0;
                    while ($pos < scalar(@parts)) {
                        my $part = $parts[$pos];
                        my $noCPPThisToken = 0;
                        if (length($part)) {
                            if (!$inChar && !$inString && !$inComment && !$inSLC) {
                                if ($parserState->{NEXTTOKENNOCPP} == 1) {
                                        # We're in an "if" block.
