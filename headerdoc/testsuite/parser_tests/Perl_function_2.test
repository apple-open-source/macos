                                                                        print STDERR "CHANGING ADDON FROM \"$addon\" TO " if ($cppDebug);
                                                                        $addon =~ s/^\s*/ /s;
                                                                        $addon =~ s/\s*$//s;
                                                                        $addon =~ s/(.)\n/$1 \\\n/sg;
                                                                        $addon =~ s/\\$/ /s;
