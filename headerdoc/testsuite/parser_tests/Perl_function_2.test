                                } elsif (!$inComment && ($part eq $ilc || $part eq $ilc_b)) {
                                        print STDERR "INSLC\n"if ($cppleaddebug);
                                        $inSLC = 1;
                                }
                            }
                            my $skip = 0;
                            if (!$incppargs) {
                                my $newpart = $part;
                                my $hasargs = 0;
                                if (!$inComment && !$inSLC && !$noCPPThisToken) {
