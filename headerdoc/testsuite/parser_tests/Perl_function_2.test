                                $rest =~ s/^\"(.*)\"$/$1/s;
                        }
                        my $filename = basename($rest);
                        if ($HeaderDoc::HeaderFileCPPHashHash{$filename}) {
                                my $includehash = HeaderDoc::IncludeHash->new();
                                $includehash->{FILENAME} = $filename;
                                $includehash->{LINENUM} = $inputCounter + $fileoffset;
