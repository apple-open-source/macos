            print STDERR "CURLINE IS \"$curline\"\n" if ($localDebug || $hangDebug);
            print STDERR "INOP: ".$parserState->{inOperator}."\n" if ($operatorDebug);

            if (!length($nextpart)) {
                print STDERR "SKIP NP\n" if ($localDebug);
                next;
            }
