  href        CDATA       #IMPLIED
  hreflang    CDATA       #IMPLIED
  type        CDATA       #IMPLIED
  rel         CDATA       #IMPLIED
  rev         CDATA       #IMPLIED
  media       CDATA       #IMPLIED
  target      CDATA       #IMPLIED
>
<!ELEMENT meta %htmlData; >
<!ATTLIST meta
  http-equiv  CDATA       #IMPLIED
  name        CDATA       #IMPLIED
  content     CDATA       #IMPLIED
