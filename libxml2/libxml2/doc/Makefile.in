distdir: $(DISTFILES)
	$(mkinstalldirs) $(distdir)/html $(distdir)/tutorial \
	   $(distdir)/tutorial/images \
	   $(distdir)/tutorial/images/callouts
	@for file in $(DISTFILES); do \
	  d=$(srcdir); \
	  if test -d $$d/$$file; then \
	    cp -pr $$d/$$file $(distdir)/$$file; \
	  else \
	    test -f $(distdir)/$$file \
	    || ln $$d/$$file $(distdir)/$$file 2> /dev/null \
	    || cp -p $$d/$$file $(distdir)/$$file || :; \
	  fi; \
	done
