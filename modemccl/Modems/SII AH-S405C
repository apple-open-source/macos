!********************************************************************!  SII PHS DATA Communication Card Series!  Ver.1.0(Ver.7.0 Base)  04/01/2003 (Masashi & Eiko Marriage version!!)!                       Written by Haruo "Hally" Kanaya!!  Ver.1.1  10/23/2003 (Hally's Gastritis version!!)!                       Written by Haruo "Hally" Kanaya!!  'mlts' resource info for this modem:!    byte 1 == 01 -> modem doesn't HAVE builtin reliability protocols!    byte 2 == 00 -> reserved by Apple!    byte 3 == 20 -> max hex chars in varstr 7 (32 dec)!    byte 4 == 00 -> max hex chars in varstr 8!    byte 5 == 00 -> max hex chars in varstr 9!********************************************************************!@ORIGINATE@ANSWER!! set up the modem - label range is 1-10!!**************************************! Mac talks to the modem at 115,200 bps.  !**************************************serreset 115200, 0, 8, 1!!**********************! reset the serial port!**********************HSReset 0 0 0 0 0 0settries 0flushDTRClearpause 10DTRSet!!@LABEL 1note "Modem Reset." 1pause 10matchclrmatchstr 1 2 "OK\13\10"write "ATZE0Q0V1X4%C3&C1&D2&K3\\V1\\X1S0=0S7=60S25=5\13"matchread 30inctriesiftries 2 90!****************! Reset the Modem!****************DTRClearpause 10DTRSetflushjump 1!!@LABEL 2pause 10!*****************************************! Modem responding & configured.! determine if reliable link is requested.!*****************************************! if modem mnp10 link requested (var 4 == 2) then jump label 5ifstr 4 5 "2"! if modem v42 link requested (var 4 == 1) then jump label 5ifstr 4 5 "1"! if no modem v42 link requested (var 4 == 0) then jump label 6ifstr 4 6 "0"! else invalid value in var 4; exit w/errorjump 95!!@LABEL 5!****************************************************! yes, v42 link is requested.  OK for LAP-M -> MNP 4.!****************************************************note "v42 link is requested.  OK for LAP-M -> MNP 4." 1matchclrmatchstr 1 10 "OK\13\10"! \N3 == v42-MNP autoreliable modewrite "AT%C3\\N3\13"matchread 30jump 90!!@LABEL 6!*******************************! yes, v42 link isn't requested.!*******************************note "v42 link isn't requested." 1matchclrmatchstr 1 10 "OK\13\10"write "AT%C0\\N0\13"matchread 30jump 90!!@LABEL 10note "Reset Complete." 1ifANSWER 30!!!****************************************************************************! modem ready, so enable answering or originate a call - label range is 11-30!****************************************************************************!@LABEL 13pause 10!! if normal dialing (parm 6 == 0) jump to 19ifstr 6 19 "0"! if blind dialing (parm 6 == 1) jump to 19ifstr 6 19 "1"! if manual dialing (parm 6 == 2) jump to 92ifstr 6 92 "2"! else invalid value in var 6; exit w/errorjump 95!@label 19! this is where we break up long dialstrings!! parm 1 is always the full dialstring from the conn docnote "Dialing ^1" 3!@label 27! parm 7 holds entire stringwrite "ATD^3^7\13"jump 30!!!*************************************!    connecting - label range is 31-60!*************************************!@LABEL 30matchclrmatchstr 1  75 "RING\13\10"matchstr 2  92 "NO CARRIER\13\10"matchstr 3  92 "ERROR\13\10"matchstr 4  93 "BUSY\13\10"matchstr 5  94 "NO ANSWER\13\10"matchstr 6  92 "DELAYED\13\10"matchstr 7  40 "CONNECT"matchread 700ifANSWER 30jump 90!!@LABEL 40matchclrmatchstr 1  41 "1200"matchstr 2  42 "2400"matchstr 3  43 "4800"matchstr 4  44 "7200"matchstr 5  45 "9600"matchstr 6  46 "12000"matchstr 7  47 "14400"matchstr 8  48 "16800"matchstr 9  49 "19200"matchstr 10 50 "32000"matchstr 11 51 "38400"matchstr 12 52 "57600"matchstr 13 53 "115200"matchread 30jump 59!!@LABEL 41CommunicatingAt 1200note "Communicating at 1200 bps" 3jump 65!@LABEL 42CommunicatingAt 2400note "Communicating at 2400 bps" 3jump 65!@LABEL 43CommunicatingAt 4800note "Communicating at 4800 bps" 3jump 65!@LABEL 44CommunicatingAt 7200note "Communicating at 7200 bps" 3jump 65!@LABEL 45CommunicatingAt 9600note "Communicating at 9600 bps" 3jump 65!@LABEL 46CommunicatingAt 12000note "Communicating at 12000 bps" 3jump 65!@LABEL 47CommunicatingAt 14400note "Communicating at 14400 bps" 3jump 65!@LABEL 48CommunicatingAt 16800note "Communicating at 16800 bps" 3jump 65!@LABEL 49CommunicatingAt 19200note "Communicating at 19200 bps" 3jump 65!@LABEL 50CommunicatingAt 32000note "Communicating at 32000 bps" 3jump 65!@LABEL 51CommunicatingAt 38400note "Communicating at 38400 bps" 3jump 65!@LABEL 52CommunicatingAt 57600note "Communicating at 57600 bps" 3jump 65!@LABEL 53CommunicatingAt 115200note "Communicating at 115200 bps" 3jump 65!@LABEL 59note "Communicating." 3jump 65!!@LABEL 60note "Communicating." 3!@LABEL 65matchclrmatchstr 1 66 "REL4\13\10"matchstr 2 66 "REL5\13\10"matchstr 3 67 "V.42\13\10"matchstr 4 67 "V.42bis\13\10"matchstr 5 70 "\10"matchread 30jump 70!@LABEL 66note "Modem Reliable Link Established." 3userhook 2jump 70!@LABEL 67note "Modem Compression Established." 3userhook 3jump 70!!!*****************************! Normal exit after "CONNECT".!*****************************!@LABEL 70! Turn on CTS handshaking.HSReset 0 1 0 0 0 0!ifANSWER 71!pause 30@LABEL 71note "Connect Complete." 1exit 0!!!*****************************! Answer calls.!*****************************!@LABEL 75ifORIGINATE 30userhook 1note "Answering phone..." 3write "ATA\13"jump 30!!!***************************************! error messages - label range is 90-100!***************************************!! Modem Not Responding@LABEL 90note "Modem Not Responding." 1exit -6019!! No Dial Tone@LABEL 91note "No Dial Tone" 1exit -6020!! No Carrier or Error@LABEL 92note "No Carrier or Error." 1exit -6021!! Busy@LABEL 93note "Busy." 1exit -6022!! No Answer@LABEL 94note "No Answer." 1exit -6023!! varstring invalid value@LABEL 95note "varstring invalid value" 1exit -6002!!!****************************************! Hang up the modem - label range is 100-!****************************************!@HANGUP@LABEL 102HSReset 0 0 0 0 0 0settries 0pause 10!@LABEL 105flushmatchclrmatchstr 1 111 "NO CARRIER\13\10"matchstr 2 109 "OK\13\10"matchstr 3 111 "ERROR\13\10"note "DTR Clear!" 1DTRSetDTRClearmatchread 30inctriesiftries 2 106note "DTR Clear Error!!" 1jump 105!@LABEL 106note "DTR Clear Error!!" 1settries 0!@LABEL 107flushmatchclrmatchstr 1 109 "OK\13\10"matchstr 2 111 "NO CARRIER\13\10"pause 30note "Send +++!" 1write "+++"matchread 30inctriesiftries 2 108note "Send +++ Error!!" 1jump 107!@LABEL 108note "Send +++ Error!!" 1settries 0!@LABEL 109pause 5flushmatchclrmatchstr 1 111 "NO CARRIER\13\10"matchstr 2 111 "OK\13\10"matchstr 3 111 "ERROR\13\10"note "Send ATH!" 1write "ATH\13"matchread 30inctriesiftries 3 111!! recall the factory settings.!@LABEL 111pause 5flushmatchclrmatchstr 1 114 "OK\13\10"note "Modem Reset." 1write "ATZS0=0\13"matchread 30jump 90!@LABEL 114note "Reset Complete." 1exit 0@LABEL 115pause 5jump 108!