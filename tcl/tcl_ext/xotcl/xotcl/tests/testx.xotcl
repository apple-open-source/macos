  errorCheck [set ::cleanupResult] \
     " ::A+->recreate ::a+::A->cleanup ::A+->recreate ::a::b+::A->cleanup" \
     "Cleanup Redefine a/a::b Failed"

  a destroy
  set ::cleanupResult ""


