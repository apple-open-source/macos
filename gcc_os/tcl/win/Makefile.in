TCLTEST		= tcltest.exe
DUMPEXTS	= # $(TMPDIR)\dumpexts.exe
TCLPIPEDLL	= cygtclpip$(VERSION).dll
TCLREGDLL	= cygtclreg$(VERSION).dll
CAT16		= # cat16.exe
CAT32		= cat32.exe

ifeq ($(OBJEXT),obj)

TCL_ALLOC_OBJ=$(TMPDIR)/tclAlloc.$(OBJEXT)

endif

