.	sp |9i-\\n(.vu
.	ps 128
\!x P gsave 0.70 setgray
\h'5.5i'\(su
\!x P grestore
.	ps
.	GB 0.75i 5.75i
.	sp |2.0i-\\n(.vu
\s36\l'5i'\s0
.	sp |2.0i-0.125i-\\n(.vu
.	ps 48
