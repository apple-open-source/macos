/* Since people link in Xmu without Xt, we avoid references to string data. */
#if HasSharedLibraries
   SHAREDCODEDEF = SharedCodeDef -DXTSTRINGDEFINES
#endif
       LINTLIBS = $(LINTXLIB) 

#if HasSnprintf
   MISC_DEFINES = -DHAS_SNPRINTF
#else
  MISC_INCLUDES = -I$(LIBSRC)/misc
#endif

