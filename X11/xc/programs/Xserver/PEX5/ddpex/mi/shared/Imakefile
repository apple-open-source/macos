#define IHaveModules
#include <Server.tmpl>

#ifndef PexDdpexCDebugFlags
#define PexDdpexCDebugFlags ServerCDebugFlags
#endif

XCOMM -D defines for ddpex:
XCOMM	DDTEST turns on some fprintf(stderr...)s for debugging
XCOMM	PEX_DEFAULT_FONTPATH defines the default path to PEX fonts

    DEFINES = PexDdpexDefines
CDEBUGFLAGS = PexDdpexCDebugFlags

  PEXSERVINC = ../../../include
DDPEXINCLUDE = ../include

INCLUDES = -I. \
	-I$(DDPEXINCLUDE) \
	-I$(XINCLUDESRC) \
	-I$(PEXSERVINC) \
	-I$(SERVERSRC)/include

SRCS = miFont.c \
	miLUT.c \
	miLineLUT.c \
	miMarkLUT.c \
	miTextLUT.c \
	miEdgeLUT.c \
	miIntLUT.c \
	miPattLUT.c \
	miFontLUT.c \
	miViewLUT.c \
	miColrLUT.c \
	miClrApLUT.c \
	miLightLUT.c \
	miDCueLUT.c \
	miNS.c \
	miUtils.c \
	miMisc.c 

OBJS = miFont.o \
	miLUT.o \
	miLineLUT.o \
	miMarkLUT.o \
	miTextLUT.o \
	miEdgeLUT.o \
	miIntLUT.o \
	miPattLUT.o \
	miFontLUT.o \
	miViewLUT.o \
	miColrLUT.o \
	miClrApLUT.o \
	miLightLUT.o \
	miDCueLUT.o \
	miNS.o \
	miUtils.o \
	miMisc.o 

ModuleObjectRule()

SubdirLibraryRule($(OBJS))

LibraryModuleTarget(ddpexs,$(OBJS))

LintLibraryTarget(dps, $(SRCS))
NormalLintTarget($(SRCS))

SpecialCObjectRule(miMisc,$(ICONFIGFILES),$(EXT_DEFINES))

DependTarget()

