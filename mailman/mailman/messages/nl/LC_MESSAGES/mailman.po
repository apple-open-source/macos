msgstr "Geen"

#: Mailman/Gui/Privacy.py:59 Mailman/Gui/Privacy.py:82
msgid "Confirm"
msgstr "Bevestiging"

#: Mailman/Gui/Privacy.py:60 Mailman/Gui/Privacy.py:83
