	\$(SHARED_LIBTOOL) --mode=link \$(LIBTOOL_MODULE_FLAG) \$(COMPILE) \$(LDFLAGS) -o \$@ -avoid-version -module -rpath \$(phplibdir) \$(LTLIBRARY_LDFLAGS) \$(LTLIBRARY_OBJECTS) \$(LTLIBRARY_SHARED_LIBADD)
	\$(SHARED_LIBTOOL) --mode=install cp \$@ \$(phplibdir)

EOF

    fi
  else
    
