LIBNAME = OSMesa

SOREV = 4

all:: lib$(LIBNAME).a

lib$(LIBNAME).a:  $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	$(AR) $@  $(OBJS)
	$(RANLIB) $@
	$(_NULLCMD_)

install:: lib$(LIBNAME).a
	@if [ -d $(DESTDIR)$(USRLIBDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(USRLIBDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(USRLIBDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(USRLIBDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) lib$(LIBNAME).a $(DESTDIR)$(USRLIBDIR)
	$(RANLIB) $(RANLIBINSTFLAGS) $(DESTDIR)$(USRLIBDIR)/lib$(LIBNAME).a

all:: lib$(LIBNAME).$(SOREV).dylib

lib$(LIBNAME).$(SOREV).dylib:   $(OBJS) $(EXTRALIBRARYDEPS)
	@MAJREV=`echo $(SOREV) | sed 's/\.[^\.]*$$//'`; \
	  INSTALLNAME=lib$(LIBNAME).$$MAJREV.dylib; \
	  set -x; (cd  .; $(CC) -o  ./$@ $(RC_CFLAGS) $(SHLIBLDFLAGS) -install_name $(USRLIBDIR)/$$INSTALLNAME -current_version $(SOREV) -compatibility_version $(SOREV) $(OBJS) $(REQUIREDLIBS) -dylib_file /System/Library/Frameworks/OpenGL.framework/Versions/A/Libraries/libGL.dylib:/System/Library/Frameworks/OpenGL.framework/Versions/A/Libraries/libGL.dylib);

cleandir::
	@MAJREV=`echo $(SOREV) | sed 's/\.[^\.]*$$//'`; \
	set -x; $(RM) lib$(LIBNAME).$$MAJREV.dylib
	$(RM) lib$(LIBNAME).$(SOREV).dylib lib$(LIBNAME).dylib

install:: lib$(LIBNAME).$(SOREV).dylib
	@if [ -d $(DESTDIR)$(SHLIBDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(SHLIBDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(SHLIBDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(SHLIBDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) lib$(LIBNAME).$(SOREV).dylib $(DESTDIR)$(SHLIBDIR)
	@MAJREV=`echo $(SOREV) | sed 's/\.[^\.]*$$//'`; \
	  set -x; $(RM) $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).$$MAJREV.dylib && \
	  $(LN) lib$(LIBNAME).$(SOREV).dylib $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).$$MAJREV.dylib
	@if $(SOSYMLINK); then (set -x; \
	  $(RM) $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).dylib; \
	  $(LN) lib$(LIBNAME).$(SOREV).dylib $(DESTDIR)$(SHLIBDIR)/lib$(LIBNAME).dylib); fi

