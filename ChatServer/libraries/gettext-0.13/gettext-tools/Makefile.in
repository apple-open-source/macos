	@$(NORMAL_UNINSTALL)
	@list='$(gettextsrc_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f $(DESTDIR)$(gettextsrcdir)/$$f"; \
	  rm -f $(DESTDIR)$(gettextsrcdir)/$$f; \
	done

