msgid "idna_to_ascii_4z: %s"
msgstr "idna_to_ascii_4z: %s"

#, c-format
msgid "idna_to_unicode_8z4z (TLD): %s"
msgstr "idna_to_unicode_8z4z (TLD): %s"

#, fuzzy, c-format
msgid "tld[%lu] = U+%04x\n"
msgstr "tld[%d] = U+%04x\n"

